{
  "mcpServers": {
    "fireproof": {
      "command": "node",
      "args": [
        "/build/index.js"
      ]
    },
    "ida-pro": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/mrexodia/ida-pro-mcp.git@f0af9fba733fb60fc13365b297b10c82db3771d7",
        "ida-pro-mcp"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@0.0.38"
      ]
    }
  }
}